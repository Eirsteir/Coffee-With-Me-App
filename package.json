  "keywords": [
    "feed",
    "newsfeed",
    "activity stream",
    "react native",
    "notification feed"
  ],
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "babel-node": "0.0.1-security",
    "babel-plugin-dotenv": "^0.1.1",
    "babel-preset-expo": "^8.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.16.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-react": "^7.20.0",
    "flow-bin": "^0.141.0",
    "flow-copy-source": "^2.0.9",
    "jest-expo": "~40.0.1",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "^2.2.1",
    "react-native-scripts": "2.0.1",
    "react-test-renderer": "17.0.1"
  },
  "main": "./index.js",
  "scripts": {
    "build": "rm -rf lib && babel '*.js' components mock --out-dir  lib && cp -R ./images/ lib/ && flow-copy-source src lib",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "eslint": "eslint '**/*.js' --max-warnings 0",
    "flow": "flow",
    "prettier": "prettier --list-different '**/*.{js,ts}'",
    "lint": "prettier --list-different '**/*.{js,ts}' && eslint '**/*.js' --max-warnings 0 && flow",
    "lint-fix": "prettier --write '**/*.{js,ts}' && eslint --fix '**/*.js'",
    "init-data": "npx babel-node scripts/initData.js",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@eva-design/eva": "^2.0.0",
    "@expo/webpack-config": "^0.12.52",
    "@ui-kitten/components": "^5.0.0",
    "axios": "^0.19.2",
